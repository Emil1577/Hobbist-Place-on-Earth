{
    "name": "hobbist-place-on-earth",
    "version": "0.1.0",
    "description": "Niche hobby ranking form that user can upvotes hobbies and populates a list of hobbies in descending order.",
    "main": "server/server.js",
    "engines":{
        "node":"16.x"
    },

    "scripts": {
        "start:dev": "concurrently \"cd client && npm run build\" \"cd server && npm run server\" ",
        "start": "node server/server.js",
        "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
        "install": "cd server && npm i --only=dev && cd ../client && npm i --only=dev",
        "seed": "cd server && npm run seed",
        "build": "cd client && npm run build",
        "server": "cd server node server.js",
        "heroku-prebuild": "npm install --dev",
        "client": "cd client && npm start"
    },
    "keywords": [
        "hobby",
        "ranked-hobbies",
        "hobbies"
    ],
    "authors": [
        "Emil Ronquillo",
        "Roksolana Odynak",
        "Henner Espinoza",
        "Jonathan Pohahau",
        "Alex Horning"
    ],
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^7.6.0"
    },
    "dependencies": {
        "dotenv": "^16.0.3",
        "heroku": "^7.3.0",
        "express": "^4.17.1",
        "concurrently": "^5.3.0",
        "css-loader": "^6.7.3",
        "html-webpack-plugin": "^5.5.0"
    }
}
